let numero = parseInt(prompt("Digite um número positivo:"));

if (numero >= 0) {
  console.log("Números ímpares de 0 até", numero, ":");
  for (let i = 0; i <= numero; i++) {
    if (i % 2 !== 0) {
      console.log(i);
    }
  }
} else {
  console.log("Número inválido! Insira um número positivo.");
}
